g_std_incl = /home/roddy/ppx_yojson_conv_lib/node_modules/bs-platform/lib/ocaml
g_pkg_flg = -bs-package-name @roddynpm/bs-easy-format
src_root_dir = /home/roddy/ppx_yojson_conv_lib/node_modules/@roddynpm/bs-easy-format
bsc = /home/roddy/ppx_yojson_conv_lib/node_modules/bs-platform/lib/bsc.exe
bsdep = /home/roddy/ppx_yojson_conv_lib/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-quiet
ppx_flags = 
g_dpkg_incls = 
g_ns = 
bsb_dir_group = 0
g_lib_incls = -I src
rule build_ast
  command = $bsc  $warnings -color always $bsc_flags -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build  src/easy_format.mlast : build_ast $src_root_dir/src/easy_format.ml
rule build_deps
  command = $bsdep -hash 7c89e8574b2f44721e75373ff43f1f81 $g_ns -g $bsb_dir_group $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/easy_format.d : build_deps src/easy_format.mlast src/easy_format.mliast
build  src/easy_format.mliast : build_ast $src_root_dir/src/easy_format.mli
rule ml_cmi
  command = $bsc -nostdlib $g_pkg_flg -color always -bs-suffix $g_lib_incls -I $g_std_incl $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/easy_format.cmi : ml_cmi src/easy_format.mliast ||  src/easy_format.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output commonjs:lib/js/src
rule ml_cmj_only
  command = $bsc -nostdlib $g_pkg_flg -color always -bs-suffix -bs-read-cmi $g_lib_incls -I $g_std_incl $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/easy_format.cmj |  $src_root_dir/lib/js/src/easy_format.bs.js : ml_cmj_only src/easy_format.mlast |  src/easy_format.cmi ||  src/easy_format.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output commonjs:lib/js/src
